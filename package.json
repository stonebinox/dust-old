{
  "name": "duskhq-web",
  "version": "1.0.0",
  "description": "DustHQ web app",
  "author": "Ignacio Rivas <rivasign@gmail.com>",
  "private": true,
  "cacheDirectories": [
    "node_modules"
  ],
  "engines": {
    "node": "8.0.x"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon -e js,ejs app.js",
    "lint": "eslint **/*.js",
    "bundle": "node fuse.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "chalk": "^2.1.0",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "ejs-blocks": "^0.1.4",
    "ejs-mate": "^2.3.0",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-flash": "^0.0.2",
    "express-session": "^1.14.2",
    "express-validator": "^3.1.2",
    "force-ssl-heroku": "^1.0.2",
    "fuse-box": "^2.2.31",
    "geoip-lite": "^1.2.1",
    "hat": "^0.0.3",
    "lob": "^3.9.0",
    "lodash": "^4.17.4",
    "lusca": "^1.4.1",
    "mapbox": "^1.0.0-beta9",
    "md5-file": "^3.2.2",
    "moment-timezone": "^0.5.13",
    "mongoose": "^4.6.6",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "nodemailer": "^3.1.7",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemon": "^1.11.0",
    "passport": "0.3.2",
    "passport-local": "^1.0.0",
    "request": "^2.78.0",
    "request-ip": "^2.0.2",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "stripe": "^5.4.0",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.1.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "arrow-body-style": 0
    },
    "env": {
      "jasmine": true,
      "mocha": true,
      "node": true
    }
  }
}
